{
  "private": true,
  "name": "capswalk",
  "version": "0.9.0",
  "description": "A multiplayer typing game for learning written languages.",
  "keywords": [
    "typing",
    "game"
  ],
  "license": "LicenseRef-LICENSE.md",
  "author": "David Fong (https://github.com/david-fong)",
  "repository": "github:david-fong/capswalk",
  "type": "commonjs",
  "files": [],
  "sideEffects": [
    "*.css",
    "index.ts"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "publisher-setup": "bash scripts/publisher-setup.sh",
    "lint": "eslint --color --cache --cache-location './node_modules/.cache/eslint-cache.json' './src/'",
    "client": "snowpack dev",
    "srv": "ts-node -T -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}' -P src/server/tsconfig.json ./src/server/index.ts",
    "srv1": "cd ./src/server && ts-node -T -r tsconfig-paths/register -O '{\"module\":\"commonjs\"}' ./index.ts",
    "bundle": "node --enable-source-maps --disable-proto=delete --title='capswalk-server' ./dist/server/pack.js"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-static-gzip": "^2.1.1",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "snowpack": "^3.0.13",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5",
    "typescript-plugin-css-modules": "^3.2.0"
  }
}
